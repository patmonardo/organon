SET c.keyPoints = ['identity of content', 'analytic proposition', 'same fact', 'cause', 'subsisting on own', 'effect', 'positedness', 'determination', 'form determinations', 'external reflection', 'tautological consideration', 'determines appearance as effect', 'rises to cause', 'same content repeated', 'nothing in cause not in effect']
