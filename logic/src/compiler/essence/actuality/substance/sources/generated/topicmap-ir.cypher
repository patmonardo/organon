SET c.description = 'In identity of cause/effect: form distinguishing (exists in itself/posited) = sublated. Cause extinguished in effect, effect extinguished (only determinateness of cause). Causality extinguished = immediacy (indifferent to relation, comes externally).'
SET c.keyPoints = ['identity of cause and effect', 'form distinguishing', 'exists in itself', 'posited', 'sublated', 'cause extinguished in effect', 'effect extinguished', 'determinateness of cause', 'causality extinguished', 'immediacy', 'indifferent to relation', 'comes externally']
