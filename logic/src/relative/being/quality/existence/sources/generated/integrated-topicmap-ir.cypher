SET c.sourceId = 'source-affirmative-infinity'
SET c.sourceFile = 'relative/being/quality/existence/sources/affirmative-infinity.txt'
SET c.lineStart = 185
SET c.lineEnd = 192
SET c.description = 'Present in both, therefore, is the same negation of negation. But this negation of negation is in itself self-reference, affirmation but as turning back to itself, that is through the mediation that the negation of negation is.'
