SET c.sourceId = 'source-infinity'
SET c.sourceFile = 'relative/being/quality/existence/sources/infinity.txt'
SET c.lineStart = 25
SET c.lineEnd = 44
SET c.description = 'It is essential to distinguish the true concept of infinity from bad infinity, the infinite of reason from the infinite of the understanding.'
