{
  "name": "dashboard-v4",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "generate": "prisma generate --no-hints --schema=./app/data/prisma/schema.prisma",
    "prisma-studio": "prisma studio --schema=./app/data/prisma/schema.prisma",
    "ps": "prisma studio",
    "b": "pnpm run build",
    "d": "pnpm run dev",
    "dx:next": "run-s migrate-dev && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "run-s generate",
    "build": "next build",
    "lint": "eslint --cache src",
    "start": "next start",
    "studio": "prisma studio --schema=./app/data/prisma/schema.prisma",
    "migrate-dev": "prisma migrate dev --schema=./app/data/prisma/schema.prisma",
    "migrate": "prisma migrate deploy --schema=./app/data/prisma/schema.prisma",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "postinstall": "prisma generate --no-hints --schema=./app/data/prisma/schema.prisma",
    "clean": "rm -rf node_modules .turbo .next .cache .output"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.66.0",
    "bcrypt": "^5.1.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "next": "^15.4.0",
    "node-fetch": "^3.3.2",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "superjson": "^1.12.4",
    "tsx": "^4.19.3",
    "use-debounce": "^10.0.4",
    "uuid": "^11.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.13.0",
    "jsdom": "^26.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prisma": "^7.2.0",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
